	"scripts": {
		"build": "concurrently \"yarn build:webpack\" \"yarn build:lib\"",
		"build:webpack": "NODE_ENV=development webpack",
		"build:lib": "NODE_ENV=development BABEL_ENV=node babel -d lib src --extensions '.ts,.tsx,.js,.jsx,'",
		"prod": "NODE_ENV=development node ./lib/server/index.js",
		"clean": "rimraf public lib",
		"test": "IS_CLIENT=false jest src",
		"check-types-t": "tsc --traceResolution",
		"check-types-w": "tsc --watch",
		"check-types": "tsc --noEmit true",
		"lint:styles": "stylelint src/components/**/*.{css,scss,js,tsx}",
		"lint:js": "eslint src/components/**/*.{js,ts,tsx}",
		"lint:html": "eslint src/helpers/Html.tsx",
		"stylelint:Loading": "stylelint src/components/Loading/Loading.tsx",
		"format": "prettier --config .prettierrc --write src/"
	},
